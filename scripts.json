{
  "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run --allow-net main.ts",
      "desc": "run the main.ts file"
    },
    "deploy": {
      "cmd": "deployctl deploy --project=wackclub --include=src ./src/main.ts",
      "desc": "deploy the project",
      "watch": false
    }
  }
}
